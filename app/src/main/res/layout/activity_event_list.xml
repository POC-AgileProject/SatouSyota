<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.PlanListActivity">

    <TextView
        android:id="@+id/textView_startYMD"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textSize="30dp"
        android:gravity="center"
        android:layout_margin="5dp"
        />

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1">


        <rd.slcs.co.jp.showtabi.view.CardRecyclerView4Event
            android:id="@+id/CardRecyclerView4Event"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></rd.slcs.co.jp.showtabi.view.CardRecyclerView4Event>


    </FrameLayout>
</LinearLayout>